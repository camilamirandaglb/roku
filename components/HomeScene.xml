<?xml version="1.0" encoding="utf-8" ?>
<component name="HomeScene" extends="Scene" >
    <interface>
        <field id="rowListContent" type="node" onchange="rowListContentChanged"/>
    </interface>
  <children>
      <Overhang showClock="true" title= "Week Two Training" optionsAvailable = "true" showOptions="true" logoUri="pkg:/images/focus_sd_logo.png" />
      <HomeList
        id= "homeList"
        itemComponentName= "HomeListItem"
        translation="[130,180]"
        itemSize = "[ 2000, 450 ]"
        showRowLabel= "[ true ]"
        drawFocusFeedback= "true"
        vertFocusAnimationStyle = "fixedFocusWrap"
        rowFocusAnimationStyle = "fixedFocusWrap"
        showRowCounter="[true , true, true]"
        visible="true"
        rowItemSize= "[[536,308], [536,308], [536,308]]"
        rowHeights= "[1000, 1000 , 1000]"
        itemSpacing= "[0, 80]"
        rowItemSpacing= "[[30, 0]]"
       />

      <Video
            id="myVideo"
            width="1920"
            height="1080"
            visible="false" />
    </children>
  <script type="text/brightscript" uri="pkg:/components/HomeScene.brs"/>
</component>
